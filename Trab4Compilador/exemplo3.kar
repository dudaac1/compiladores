void main() {
    newVar integer num1;
    newVar integer num2;
    num1 = 0;
    num2 = 50;

    repeat {
        num1 = (num1 + 1);
        num2 = (num2 - 2);
    }
    until (teste(num1, num2));
}

func bool teste(integer n1, integer n2) {
    newVar bool result;
    if ((n1 > n2)) then {
        result = true;
    };
    if ((n1 < n2)) then {
        result = false;
    };
    return result;
}